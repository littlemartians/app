{
  "name": "littlemartians-subgraph-local",
  "license": "MIT",
  "scripts": {
    "refresh-local": "node src/scripts/getAbis.js && node src/scripts/getDeployments.js",
    "prep": "node src/scripts/prepare.js",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy-goerli": "graph deploy --node https://api.studio.thegraph.com/deploy/ eden-livemint-dev",
    "create-local": "graph create --node http://localhost:8020/ littlemartians-subgraph-local",
    "remove-local": "graph remove --node http://localhost:8020/ littlemartians-subgraph-local",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 littlemartians-subgraph-local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.33.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "mustache": "^4.0.1"
  },
  "devDependencies": {
    "fs": "^0.0.1-security",
    "js-yaml": "^4.1.0"
  }
}
